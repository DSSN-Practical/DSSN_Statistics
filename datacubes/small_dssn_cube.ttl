# -- Data Sets --------------------------------------------
xo:dataset-xoFollower a qb:DataSet;
#--xo dataset for follower
    rdfs:label      "Follower"@en;
    rdfs:comment    "Number of followers of one dssn node"@en;
    dct:publisher   xo:team-dssnpWiSe2014 ;
    dct:issued      "2014-02-01"^^xsd:date;
    qb:structure xo:dsd-xoFollower ;
    sdmx-attribute:unitMeasure xsd:integer
    .


# -- Data structure definition ----------------------------

# pro messgröße eine DataStructureDefinition
xo:dsd-xoFollower a qb:DataStructureDefinition;
    qb:component
    # The dimensions
        [ qb:dimension xo:refAgent;      qb:order 1 ],
        [ qb:dimension xo:refTime;       qb:order 2; qb:componentAttachment qb:Slice ];

    # The measure(s)
    qb:component [ qb:measure [xo:follower a qb:MeasureProperty]];

    # The attributes
    qb:component [ qb:attribute sdmx-attribute:unitMeasure;
                   qb:componentRequired "true"^^xsd:boolean;
                   qb:componentAttachment qb:DataSet; ]
    .

#-------------------------components
xo:CSrefAgent  a rdf:Property, qb:ComponentSpecification; 
    rdfs:label "Xodx Agent Component"@en;
    qb:dimension xo:refAgent ;
    .

xo:CSrefTime  a rdf:Property, qb:ComponentSpecification;
    rdfs:label "reference Time Component"@en;
    qb:dimension xo:refTime ;
    .

xo:CSfollower a rdf:Property, qb:ComponentSpecification;
    rdfs:label "number of followers Component"@en;
    qb:attribute xo:unit ;
    .

xo:CSvalue a rdf:Property, qb:ComponentSpecification;
    rdfs:label "Value Measure Component"@en;
    qb:measure xo:value ;
    .

#-----------dimensions, units and measure

xo:refAgent a qb:DimensionProperty ;
    rdfs:label "Agent Dimension"@en .

xo:refTime a qb:DimensionProperty ;
    rdfs:label "Time Dimension"@en .

xo:unit a qb:AttributeProperty ;
    rdfs:label "Another Unit" .

xo:value a qb:MeasureProperty ;
    rdfs:subPropertyOf sdmx-measure:obsValue;
    rdfs:label "Value"@en .

#-------------------------observations
xo:o01 a qb:Observation;
    qb:dataSet  xo:dataset-xoFollower ;
    xo:refAgent xo:agent01 ;
    xo:refTime  1 ;                  
    xo:value 20 ;
    .
