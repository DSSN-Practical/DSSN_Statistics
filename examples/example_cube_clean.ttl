    exProp:value "2501"^^<http://www.w3.org/2001/XMLSchema#float> ;
    cube:dataSet ex:dataset3 ;
    a cube:Observation .

exProp:anotherUnit
    a cube:AttributeProperty ;
    rdfs:label "Another Unit" .

exProp:date
    a cube:DimensionProperty ;
    rdfs:label "Date"@en .

exProp:geo
