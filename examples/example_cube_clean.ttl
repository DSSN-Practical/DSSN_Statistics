    exProp:geo ex:Germany ;
    exProp:unit "whatever it is about" ;
    exProp:value "1998"^^<http://www.w3.org/2001/XMLSchema#float> ;
    cube:dataSet ex:dataset2 ;
    a cube:Observation .

ex:obs7
