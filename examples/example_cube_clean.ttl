    exProp:geo ex:England ;
    exProp:unit "whatever it is about" ;
    exProp:value "1999"^^<http://www.w3.org/2001/XMLSchema#float> ;
    cube:dataSet ex:dataset ;
    a cube:Observation ;
    rdfs:label "England in 2002" .

ex:obs5
