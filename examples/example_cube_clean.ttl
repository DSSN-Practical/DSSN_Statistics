    exProp:geo ex:Germany ;
    exProp:unit "whatever it is about" ;
    exProp:value "2000"^^<http://www.w3.org/2001/XMLSchema#float> ;
    cube:dataSet ex:dataset ;
    a cube:Observation ;
    rdfs:label "Germany in 2002" .

