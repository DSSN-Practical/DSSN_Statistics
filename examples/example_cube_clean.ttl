    exProp:geo ex:Germany ;
    exProp:unit "plain number" ;
    exProp:value "1998"^^<http://www.w3.org/2001/XMLSchema#float> ;
    cube:dataSet ex:dataset3 ;
    a cube:Observation ;
    rdfs:label "Germany in 2003" .

ex:obs8
